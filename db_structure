- user (id, name)
- field (id, title, description, image, status)
- user_field (id, user_id, field_id, user_title, user_description, user_relation, doing_from, learning_from)
- educational_institution (id, title, description, type, link, status)
- degree (id, educational_institution_id, title, description, type, link, status)
- user_degree (id, degree_id, start_date, end_date, avg_score, comment, status)
- field_degree (id, field_id, degree_id)
- user_field_degree (id, user_field_id, user_degree_id)
- additional_education (id, title, description, author, type, link, status)
- user_additional_education (id, additional_education_id, start_date, end_date, comment, status)
- field_additional_education (id, field_id, additional_education_id)
- user_field_additional_education (id, user_field_id, user_additional_education_id)
- book (id, title, description, author, image, link, status)
- user_book (id, start_date, end_date, comment, status)
- field_book (id, field_id, book_id)
- user_field_book (id, user_field_id, user_book_id)
- material (id, title, description, author, type, link, status)
- user_material (id, start_date, end_date, comment, status)
- field_material (id, field_id, material_id)
- user_field_material (id, user_field_id, user_material_id)
- continuous_resource (id, title, description, author, type, link, status)
- user_continuous_resource (id, start_date, end_date, comment)
- field_continuous_resource (id, field_id, continuous_resource_id)
- user_field_continuous_resource (id, user_field_id, user_continuous_resource_id)
- skill (id, title, description, status, parent_id)
- user_skill (id, skill_id, user_title, user_description, user_relation, doing_from, learning_from)
- field_skill (id, field_id, skill_id)
- user_field_skill (id, user_field_id, user_skill_id)
